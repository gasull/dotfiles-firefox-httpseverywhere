<ruleset name="Google to Startpage">
  <target host="www.google.ch" />
  <target host="www.google.com" />
  <target host="www.google.es" />
  <target host="www.google.ro" />
  <target host="google.ch" />
  <target host="google.com" />
  <target host="google.es" />
  <target host="google.ro" />
  <target host="encrypted.google.com" />
  <rule
      from="^http(s?)://(www\.|encrypted\.)?google\.(ch|com|es|ro)/(search\?)?(hl=en&amp;)?#?q="
      to="https://startpage.com/do/search?query="
  />
</ruleset>
